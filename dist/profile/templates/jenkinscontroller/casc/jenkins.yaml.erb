<%- if @jcasc['jenkins_global'] && !@jcasc['jenkins_global']['disabled'] == "true" -%>
jenkins:
  # No builds on controller
  numExecutors: 0
  globalNodeProperties:
  - envVars:
      env:
      - key: "AZURE_COLLECT_TELEMETRY"
        value: "false"
unclassified:
  shell:
    shell: "/bin/bash"
<%- end -%>
